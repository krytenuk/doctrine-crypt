{
  "name": "krytenuk/doctrine-crypt",
  "description": "Laminas module for automatic encrypting/decrypting data in Doctrine entities",
  "require": {
    "php": "^8.0",
    "symfony/console": "^5.4 || ^6.0",
    "doctrine/doctrine-orm-module": "^4.1 || ^5.1",
    "laminas/laminas-crypt": "^3.6",
    "laminas/laminas-eventmanager": "^3.4",
    "laminas/laminas-modulemanager": "^2.11",
    "laminas/laminas-stdlib": "^3.6",
    "laminas/laminas-hydrator": "^4.13",
    "ext-gettext": "*",
    "ext-openssl": "*"
  },
  "authors": [
    {
      "name": "Garry Childs",
      "email": "info@freedomwebservices.net",
      "homepage": "https://www.freedomwebservices.net/laminas/fws-doctrine-auth"
    }
  ],
  "keywords": [
    "Laminas",
    "encryption",
    "decryption",
    "doctrine",
    "ORM",
    "freedom"
  ],
  "autoload": {
    "psr-4": {
      "FwsDoctrineCrypt\\": "src/"
    }
  },
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/krytenuk/fws-doctrine-auth",
  "extra": {
    "zf": {
      "module": "FwsDoctrineCrypt"
    }
  },
  "funding": [
    {
      "type": "other",
      "url": "https://www.freedomwebservices.net/laminas/fws-doctrine-auth#donate"
    }
  ]
}
